@article{anwar_sectoral_2024,
	title = {Do sectoral training programs reduce arrests?: Evidence from a low-income targeted training program RCT},
	shorttitle = {Do Sectoral Training Programs Reduce Arrests?},
	html = {https://jhr.uwpress.org/content/early/2024/12/02/jhr.1122-12629R4.abstract},
	doi = {10.3368/jhr.1122-12629R4},
	abstract = {This study presents the first causal evidence of the impact on arrest rates of a sectoral training program that was implemented using a randomized control trial. The results indicate that, among those with a prior criminal record, individuals in the treatment group were two-fifths as likely to be arrested post-randomization as those assigned to the control group. Among the overall population, arrest probabilities decreased by one-half. We explore potential mechanisms for this effect and find suggestive evidence in favor of the training program’s impact on earnings, as well as peer effects from other trainees.},
	urldate = {2024-12-31},
	journal = {Journal of Human Resources},
	author = {Anwar, Shamena and Baird, Matthew and Engberg, John and Smart, Rosanna},
	year = {2024},
	preview={anwar_sectoral_2024.png},
}

